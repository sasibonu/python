
def minion_game(string):


    vowels = 'AEIOU'
    ks = 0
    sts = 0
    for i in range(len(s)):
        if (s[i] in vowels):
            ks += (len(s)-i)
        else:
            sts += (len(s)-i)

    if ks > sts:
        print ("Kevin", ks)
    elif ks < sts:
        print ("Stuart", sts)
    else:
        print ("Draw")
    
if __name__ == '__main__':
s = input()
minion_game(s)

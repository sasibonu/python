a,b = input().split()
h=0
l1= list()
l2= list()
l3= list()
n = input().split()
l1.append(n)
p = input().split()
l2.append(p)
q = input().split()
l3.append(q)
for i in range(0,int(a)):
    for j in range(0,int(b)):
        if (l2[j]==l1[i]):
            h=h+1
        elif (l3[j]==l1[i]):
            h=h-1
        else:
            h=h+0
print(h)


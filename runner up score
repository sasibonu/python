if __name__ == '__main__':
    n = int(input())
    a = map(int, input().split())
    a=[int(i) for i in a]
    i=1
    for i in range(n):
        t=a[i-1]
        if a[i]>a[i-1]:
            t=a[i]
    a=list(filter(lambda i: i != t, a))
    n1=len(a)
    i=1
    for i in range(n1):
        r=a[i-1]
        if a[i]>a[i-1]:
            r=a[i]
    print(r)     
